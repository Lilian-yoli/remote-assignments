function twoSum(nums, target) {
// your code here
  let obj = {}

  for(let i = 0; i < nums.length; i++){
    let searchValue = target - nums[i]
    let index = obj[searchValue]

    if (obj[searchValue] != null) {
      return [index, i]
    } else {
      obj[nums[i]] = i
    }
  }
}

/*
For example:
twoSum([2, 7, 11, 15], 9); Should returns:
[0, 1] Because:
nums[0]+nums[1] is 9
*/
